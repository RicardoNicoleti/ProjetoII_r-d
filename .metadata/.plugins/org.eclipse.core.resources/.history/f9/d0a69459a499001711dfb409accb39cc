package br.edu.facear.classes;

import java.util.List;

public class Ranking {
	private int semanal;
	private int mensal;
	private List<Jogador> jogador;
	
	public void GerenciarRanking(){
		
		for (Jogador jogador : jogador) {
			System.out.println(jogador);
		}	
		
	}
	
	
	public Ranking(int semanal, int mensal, List<Jogador> jogador) {
		this.semanal = semanal;
		this.mensal = mensal;
		this.jogador = jogador;
	}

	public Ranking(int semanal, int mensal) {
		this.semanal = semanal;
		this.mensal = mensal;
	}
	public int getSemanal() {
		return semanal;
	}
	public void setSemanal(int semanal) {
		this.semanal = semanal;
	}
	public int getMensal() {
		return mensal;
	}
	public void setMensal(int mensal) {
		this.mensal = mensal;
	}

	public List<Jogador> getJogador() {
		return jogador;
	}

	public void setJogador(List<Jogador> jogador) {
		this.jogador = jogador;
	}
	
	
}
