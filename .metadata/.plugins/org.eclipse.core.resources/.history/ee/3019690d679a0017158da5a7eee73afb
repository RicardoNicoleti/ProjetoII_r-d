package br.edu.facear.telas;

import java.util.ArrayList;
import java.util.List;

import br.edu.facear.classes.Categoria;
import br.edu.facear.classes.Jogador;
import br.edu.facear.classes.Ranking;

public class Principal {

	public static void main(String[] args) {
		
		//Instancia
		Jogador[] jogador = new Jogador[2];
		Categoria c = new Categoria();
		
		//Atribuição
		
		jogador[0].setNome("Ricardo");
		jogador[0].setIdade(27);
		jogador[0].setTelefone(99999999);
		jogador[0].setNivel(3);
		jogador[0].setLogin("admin");
		jogador[0].setSenha("123");

		
		
		jogador[1].setNome("João");
		jogador[1].setIdade(27);
		jogador[1].setTelefone(99999999);
		jogador[1].setNivel(3);
		jogador[1].setLogin("admin");
		jogador[1].setSenha("145");
		 
				
		
		for (int i = 0; i < jogador.length; i++) {
			System.out.println(jogador[i]);
		}
		
		Ranking r = new Ranking();
		r.GerenciarRanking();
						
	}
}
